{
  "extends": "airbnb-base",
  "root": true,
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "func-names": "off",
    "object-curly-newline": "off",
    "consistent-return": "off",
    "linebreak-style": "off",
    "no-console": "off",
    "no-alert": "off",
    "quotes": ["error", "double"],
    "import/no-cycle": "error",
    "max-len": ["warn", { "code": 120 }],
    "no-underscore-dangle": ["error", {"allow": ["_id"]}],
    "wrap-iife": ["warn", "outside"],
    "comma-dangle": ["error", "never"]
  },
  "env": {
    "browser": true,
    "es6": true,
    "greasemonkey": true
  }
}
